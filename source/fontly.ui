  <property name="styleSheet">
   <string notr="true">/* FONT */
/* app name */
QLabel#fontlyLabel{
	font: 75 18pt &quot;Roboto&quot;;
}
/* text inputs */
QLineEdit#fontApply, QLineEdit#search{
	font: 57 14pt &quot;Roboto&quot;;
}
/* sections */
QLabel#menuLabel , QLabel#socialLabel {
	font: 57 13pt &quot;Roboto Medium&quot;;
}
/* subsections */
QLabel#fontPickerLabel, QLabel#myFontsLabel, QLabel#settingsLabel, QFrame#githubLabel, QLabel#facebookLabel, QLabel#websiteLabel  {
	font: 75 13pt &quot;Roboto&quot;;
}

/* ICONS */
QPushButton#close {
	image: url(:/assets/icons/closeINNACTIVE.png);
}
QPushButton#maximize {
	image: url(:/assets/icons/maximizeINNACTIVE.png);
}
QPushButton#minimize {
	image: url(:/assets/icons/minimizeINNACTIVE.png);
}
QPushButton#textColor {
	image: url(:/assets/icons/textColor.png);
}
QPushButton#textBackgroundColor {
	image: url(:/assets/icons/textBackgroundColor.png);
}
QPushButton#resetSettings {
	image: url(:/assets/icons/resetSettings.png);
}
QFrame#fontPickerIcon {
	image: url(:/assets/icons/fontpicker.png);
}
QFrame#myFontsIcon {
	image: url(:/assets/icons/myfonts.png);
}
QFrame#settingsIcon {
	image: url(:/assets/icons/settings.png);
}
QFrame#socialGithubIcon {
	image: url(:/assets/icons/socialGITHUB.png);
}
QFrame#socialFacebookIcon {
	image: url(:/assets/icons/socialFB.png);
}
QFrame#socialWebsiteIcon {
	image: url(:/assets/icons/socialWWW.png);
}
QFrame#welcomeWindowBackground {
	image: url(:/assets/icons/welcomeWindow.png);
}
QFrame#searchIconBackground {
	image: url(:/assets/icons/search.png);
}
/* BACKGROUND  COLOR */

/* transparency */
QFrame, QPushButton{
	background-color: rgba(0,0,0,0);
}
/* main color: darker */
QFrame#actionButtonsArea {
	background-color: #263238;
}
/* main color: lighter */
QFrame#toolsArea, QFrame#bottomBorder, QFrame#leftPanelArea{
	background-color: #37474f;
}
/* white */
QTextEdit, QLineEdit, QListWidget#textScroll, QFrame#appBackgroundPanel{
	background-color: rgb(255,255,255);
}

/* accent elements */
QFrame#searchIconBackground, QFrame#appNameBackground{
	background-color: #e74c3c;
}

/* TEXT COLOR */

/* white */
QLabel#fontlyLabel {
  color:  #fff;
}

/* white 50% opacity */
QLabel#myFontsLabel, QLabel#settingsLabel, QFrame#githubLabel, QLabel#facebookLabel, QLabel#websiteLabel , QLabel#fontPickerLabel, QLabel#menuLabel , QLabel#socialLabel {
  color: rgba(255, 255, 255, 0.5);
}
/* BORDER COLOR */

/* transparent */
QPushButton {
	border: 1px solid rgba(0,0,0,0);
}
/* BORDER RADIUS */

/* both sides */
QLineEdit#fontApply, QFrame#sortPanel {
    border-radius: 20%;
}
/* right side */
QFrame#searchIconBackground, QPushButton#sortByDate{
	border-top-right-radius: 20%;
	border-bottom-right-radius: 20%;
}
QFrame#appNameBackground {
	border-top-right-radius: 25%;
	border-bottom-right-radius: 25%;
}
/* left side */
QLineEdit#search, QPushButton#sortByPopularity {
	border-color:rgba(0,0,0,0);
	border-top-left-radius: 20%;
	border-bottom-left-radius: 20%;
}

/* SLIDER */
QSlider::groove:horizontal {
	border-radius: 4px;
    height: 8px;
    background-color: #263238;
}

QSlider::handle:horizontal {
    background-color: #e74c3c;
    border: 1px solid #e74c3c;
    width: 15px;
	margin: -4px 0;
    border-radius: 4px;
}

/* ON HOVER */
QPushButton#fontPickerButton:hover, QPushButton#myFontsButton:hover, QPushButton#settingsButton:hover, QPushButton#githubButton:hover, QPushButton#facebookButton:hover, QPushButton#websiteButton:hover{
	border-left: 2px solid rgba(255,255,255,0.5);
}
QPushButton#fontPickerButton:pressed, QPushButton#myFontsButton:pressed, QPushButton#settingsButton:pressed,QPushButton#githubButton:pressed, QPushButton#facebookButton:pressed, QPushButton#websiteButton:pressed {
	border-left: 2px solid rgba(255,255,255,1);
}
QPushButton#close:hover {
	image: url(:/assets/icons/close.png);
}

QPushButton#maximize:hover {
	image: url(:/assets/icons/maximize.png);
}

QPushButton#minimize:hover {
	image: url(:/assets/icons/minimize.png);
}

QPushButton#close:pressed {
	image: url(:/assets/icons/closeINNACTIVE.png);
}

QPushButton#maximize:pressed {
	image: url(:/assets/icons/maximizeINNACTIVE.png);
}

QPushButton#minimize:pressed {
	image: url(:/assets/icons/minimizeINNACTIVE.png);
}
</string>

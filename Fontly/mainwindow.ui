  <property name="styleSheet">
   <string notr="true">/* FONT *

/* app name */
QLabel#fontlyLabel{
	font: 57 18pt &quot;Lato Medium&quot;;
}
/* sections */
QLabel#menuLabel , QLabel#socialLabel {
	font: 63 14pt &quot;Lato Semibold&quot;;
}
/* subsections */
QLabel#fontPickerLabel, QLabel#myFontsLabel, QLabel#settingsLabel, QFrame#githubLabel, QLabel#facebookLabel, QLabel#websiteLabel  {
	font: 63 14pt &quot;Lato Semibold&quot;;
}
/* author */
QLabel#authorLabel {	
	font: 57 8pt &quot;Lato Medium&quot;;
}

/* ICONS */
QPushButton#close {
	image: url(:/rsc/close.png);
}
QPushButton#maximize {
	image: url(:/rsc/maximize.png)
}
QPushButton#minimize {
	image: url(:/rsc/minimize.png);
}
QPushButton#textColor {
	image: url(:/rsc/textColor.png);
}
QPushButton#textBackgroundColor {
	image: url(:/rsc/textBackgroundColor.png);
}
QPushButton#resetSettings {
	image: url(:/rsc/resetSettings.png);
}
QFrame#fontPickerIcon {
	image: url(:/rsc/fontpicker.png);
}
QFrame#myFontsIcon {
	image: url(:/rsc/myfonts.png);
}
QFrame#settingsIcon {
	image: url(:/rsc/settings.png);
}
QFrame#socialGithubIcon {
	image: url(:/rsc/socialGITHUB.png);
}
QFrame#socialFacebookIcon {
	image: url(:/rsc/socialFB.png);
}
QFrame#socialWebsiteIcon {
	image: url(:/rsc/socialWWW.png);
}

/* BACKGROUND  COLOR */

/* transparency */
QFrame{
	background-color: rgba(0,0,0,0);
}
/* main color: darker */
QFrame#actionButtonsArea {
	background-color: rgb(38, 50, 56);
}
/* main color: lighter */
QFrame#authorArea, QFrame#toolsArea {
	background-color: rgb(55, 71, 79);
}
/* white */
QTextEdit, QLineEdit, QListWidget#textScroll {
	background-color: rgb(255,255,255);
}
/* white but darkerr */
QFrame#leftPanelArea {
	background-color: rgb(245, 245, 245);
}
/* accent elements */
QFrame#searchIconBackground, QFrame#appNameBackground{
	background-color: #e74c3c;
}

/* TEXT COLOR */

/* white */
QLabel#fontlyLabel , QLabel#authorLabel {
  color:  #fff;
}
/* black 50% opacity */
QLabel#menuLabel , QLabel#socialLabel {
  color: rgba(0,0,0,.5);
}
/* black 35% opacity */
QLabel#fontPickerLabel, QLabel#myFontsLabel, QLabel#settingsLabel, QFrame#githubLabel, QLabel#facebookLabel, QLabel#websiteLabel  {
  color: rgba(0,0,0,.35);
}
/* BORDER COLOR */

/* transparent */
QListView {
	border: 1px solid rgba(0,0,0,0);
}
/* BORDER RADIUS */

/* both sides */
QLineEdit#fontApply {
    border-radius: 20%;
}
/* right side */
QFrame#searchIconBackground {
	border-top-right-radius: 20%;
	border-bottom-right-radius: 20%;
}
QFrame#appNameBackground {
	border-top-right-radius: 25%;
	border-bottom-right-radius: 25%;
}
/* left side */
QLineEdit#search{
	border-color:rgba(0,0,0,0);
	border-top-left-radius: 20%;
	border-bottom-left-radius: 20%;
}

/* SLIDER */
QSlider::groove:horizontal {
	border-radius: 4px;
    height: 8px;
    background-color: #263238;
}

QSlider::handle:horizontal {
    background-color: #e74c3c;
    border: 1px solid #e74c3c;
    width: 15px;
	margin: -4px 0;
    border-radius: 4px;
}

</string>
